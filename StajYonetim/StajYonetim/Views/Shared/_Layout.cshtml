
<!DOCTYPE html>

<html>
<head>
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/CSS/Style.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,400,300,600&subset=latin,latin-ext' rel='stylesheet' type='text/css'>
</head>
<body>
    <div id="container">
        <header>ALTAY MESLEKİ VE TEKNİK ANADOLU LİSESİ <p></p>STAJ YÖNETİM SİSTEMİ</header>

        <nav>
            <a href="/">
                <div class="nav_item first">
                    <span>Anasayfa</span>
                </div>
            </a>
            <div class="nav_item">
                <span>Okul Girişi</span>
            </div>
            <div class="nav_item">
                <span>Öğrenci Girişi</span>
            </div>
            <div class="nav_item">
                <span>Öğretmen Girişi</span>
            </div>
            <div class="nav_item">
                <span>İşletme Girişi</span>
            </div>
            <div class="nav_item">
                <span>Belgeler</span>
            </div>
            <a href="/iletisim/">
                <div class="nav_item last">
                    <span>İletişim</span>
                </div>
            </a>
        </nav>
        <div id="main">
            <div id="left">
                <form id="login" action="#" method="post">
                    <div class="divBaslik">KULLANICI GİRİŞİ</div>
                    <div class="login_line">
                        <div class="login_col1"></div>
                        <div class="login_col2">
                           
                            <input type="button" class="Button" id="gonder" name="Gonder" value="Giriş" onclick="location.href='@Url.Action("SignIn", "Account" )'"/>
                        </div>
                    </div>
                </form>
                <div id="news"><div class="divBaslik">DUYURULAR</div></div>
            </div>
            <div id="content">
                @RenderBody()
            </div>
        </div>
        <footer><small> &copy; Sultan YILDIZ DEMİR | Tüm hakkı saklıdır. </small></footer>
    </div>
</body>
</html>